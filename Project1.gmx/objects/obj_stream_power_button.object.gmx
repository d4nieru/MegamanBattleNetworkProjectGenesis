            <string>///Enabling the button
switch(Button){
    case "Off":
    image_index = 0;
    break;
    
    case "On":
    image_index = 1;
    break;
}

switch(Effect){
    case "Heel":
    if obj_player.sb_count &gt; 0{
    for(i=1;i&lt;4;i++){
        if pc.ChipSlot[i,pc.Code] != "0" &amp;&amp; pc.ChipSlot[i,pc.Code] != "" &amp;&amp; pc.ChipSlot[i,pc.Code] = pc.ChipSlot[i+1,pc.Code]{
            Button = "On";
            exit;
        }
        Button = "Off";
    }
    }
    break;
    
    case "Erase":
    if obj_player.sb_count &gt; 0{
    if pc.ChipSlot[1,pc.Name] = "Invis" &amp;&amp; pc.ChipSlot[2,pc.Element] = CElement.Cursor{
        Button = "On";
        exit;
    }
    Button = "Off";
    }
    break;
    
    case "Number":
    if obj_player.sb_count &gt; 0 &amp;&amp; instance_number(obj_chip) &gt;= 3{
    if pc.ChipSlot[1,pc.Element] = CElement.Plus &amp;&amp; pc.ChipSlot[2,pc.Element] = CElement.Plus{
        Button = "On";
        exit;
    }
    Button = "Off";
    }
    break;
    
    case "Napalm":
    if obj_player.sb_count &gt; 0{
        if pc.ChipSlot[1,pc.Element] = CElement.Fire{
            for(i=1; i&lt;13; i++){
                if string_char_at(pc.ChipSlot[2,pc.Name],i) = "B"{
                    if string_char_at(pc.ChipSlot[2,pc.Name],i+1) = "o"{
                        if string_char_at(pc.ChipSlot[2,pc.Name],i+2) = "m"{
                            if string_char_at(pc.ChipSlot[2,pc.Name],i+3) = "b"{
                                Button = "On";
                                exit;
                            }
                        }
                    }
                }
            }
        }
        Button = "Off";
    }
    break;    
}

</string>
