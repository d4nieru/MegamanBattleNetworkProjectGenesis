    image_index = 0;
    ShakeTimer = 20;
    FlashTimer = 180;
    f = -0.05;
    savex = x;
    savey = y;
    break;
    
    case "Light-Heavy":
    HitStun = "";
    Protection = "Invis";
    sprite_index = Hurt_S;
    image_index = 0;
    DamageTimer = 20;
    FlashTimer = 180;
    f = -0.05;
    break;       
    
    case "Medium":
    HitStun = "";
    sprite_index = Hurt_S;
    image_index = 0;
    if ShakeTimer = 0{
        savex = IP.x;
        savey = IP.y;
    }
    ShakeTimer = 20;
    break;    
}

if DamageTimer &gt; 0{
    DamageTimer -= 1;
    if DamageTimer = 1 || DamageTimer = 0{
        image_index = 0.1;
        DamageTimer = 0;
        exit;
    }
}

if ShakeTimer &gt; 0{
    ShakeTimer -= 1;
    if ShakeTimer = 1 || ShakeTimer = 0{
        x = savex;
        y = savey;
        image_index = 0.1;
        ShakeTimer = 0;
        exit;
    }
    rx = choose(1,2,3,0,-1,-2,-3,);
    ry = choose(1,2,3,0,-1,-2,-3,);
    x = savex+rx;
    y = savey+ry;
}

if FlashTimer &gt; 0 &amp;&amp; HP &gt; 0{
    FlashTimer -= 1;
    if FlashTimer = 0{
        image_alpha = 1;
        Protection = "None";
        exit;
    }
    image_alpha += f;
    if image_alpha = .25 &amp;&amp; f &lt; 0{
        f = f*-1;
        exit;
    }
    if image_alpha = .5 &amp;&amp; f &gt; 0{
        f = f*-1;
        exit;
    }
}


}
