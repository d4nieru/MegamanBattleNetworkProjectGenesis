          <argument>
            <kind>1</kind>
            <string>CID = Show_Chip_ext[0];
if keyboard_check_pressed(vk_down){
     if Position != 5{
         Position += 1;
         Show_Chip_ext[0] += 1;
         exit;
     }
     if Position = 5 &amp;&amp; Show_Chip_ext[0] != 29{
        Position = 1;
        Show_Chip_ext[0] += 1;
        for (j=0; j&lt;5; j+=1){
           Show_Chip[j] += 5
        }
        exit;
     }
}
if keyboard_check_pressed(vk_up){
     if Position != 1{
         Position -= 1;
         Show_Chip_ext[0] -= 1; 
         exit;
     }
     if Position = 1 &amp;&amp; Show_Chip_ext[0] != 0{
        Position = 5;
        Show_Chip_ext[0] -= 1;
        for (j=0; j&lt;5; j+=1){
           Show_Chip[j] -= 5
        }        
        exit;
     }
}

if keyboard_check_pressed(ord("Z")) &amp;&amp; global.Saved_Chip != -4{
      for(i = 0; i &lt; 99; i += 1;){
          if CTO[i] = noone{
               CTO[i] = Chip_Deck[CID];
               CTA[i] += 1;
               break;
          }
          if CTO[i] = Chip_Deck[CID]{
             CTA[i] += 1
             break;
          }
      }      
      Chip_Deck[CID] = global.Saved_Chip;
      with(instance_create(0,0,global.Saved_Chip)){
           other.Chip_ID[other.CID] = id;
           Code_A = global.Saved_Code;
      }
      /*Chip_ID[Position-1].Name = global.Saved_Name;
      Chip_ID[Position-1].Code_A = global.Saved_Code;
      Chip_ID[CID].Card_Pic = global.Saved_Pic;  */
      global.Saved_Chip = -4;
}
</string>
