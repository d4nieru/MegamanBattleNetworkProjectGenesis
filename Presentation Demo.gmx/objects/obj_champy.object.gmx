    Attack_Timer -= 1;
    if Attack_Timer &lt;= 0{
        if !place_meeting(obj_megaman.x+40,y,obj_virus) &amp;&amp; instance_place(obj_megaman.x+40,y,obj_panel).Panel_Status != 3 &amp;&amp; instance_place(obj_megaman.x+40,y,obj_panel).Panel_Status != 4{
          with(instance_create(IP.x,IP.y,obj_virus_placeholder)){
             Owner = other.id;
             other.VChild = id;
         }            
            SaveX = x;
            SaveY = y;
            Attack_Timer = MAT;
            x = instance_place(obj_megaman.x+40,y,obj_panel).x-5
            Uppercut = 1;
        } else Attack_Timer = MAT;
    }
}    if Diff != 2 {Attack_Timer = MAT;}

 if Uppercut{  
